menuCurrentHeight = 96;

///show or hide the corr. part of child according to currentTop and childPerPage
for(i = 0; i &lt; childCount; i ++){
    if(i &lt; currentTop or i &gt; max(currentTop + childPerPage - 1,0)){
        //show_message("deactivate " + string(i));
        with (child[i]){
            instance_deactivate_object(display);
            instance_deactivate_object(save);
        }
        instance_deactivate_object(child[i]);
    }
    else {
        //show_message("activate " + string(i));
        instance_activate_object(child[i]);
        with (child[i]){
            x = other.x;
            y = other.menuCurrentHeight + (other.i - other.currentTop) * other.margin;
            instance_activate_object(display);
            instance_activate_object(save);
            display.x = x + 20;
            display.y = y + 10 + 20;
            save.x = x + objGenBlockMenu.menuWidth - 50 - 32;
            save.y = y + 10 + 16 + 20;
        }
    }
}
